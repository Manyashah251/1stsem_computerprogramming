#include <stdio.h>

int main() {
    int i, n, sum = 0;

    // Input value of n
    printf("Enter the value of n: ");
    scanf("%d", &n);

    // Calculate sum of squares
    for(i = 1; i <= n; i++) {
        sum = sum + (i * i);  // add square of each number
    }

    // Output result
    printf("Sum of squares of first %d natural numbers = %d\n", n, sum);

    return 0;
}
