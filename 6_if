#include <stdio.h>

int main() {
    int m1, m2, m3, total;
    float avg;

    // Input marks of three subjects
    printf("Enter marks of 3 subjects: ");
    scanf("%d %d %d", &m1, &m2, &m3);

    // Calculate total and average
    total = m1 + m2 + m3;
    avg = total / 3.0;

    // Check if student failed in any subject
    if(m1 < 35 || m2 < 35 || m3 < 35) {
        printf("Total = %d, Average = %.2f\n", total, avg);
        printf("Result: Fail (scored less than 35 in a subject)\n");
    }
    else {
        printf("Total = %d, Average = %.2f\n", total, avg);

        if(avg >= 70) {
            printf("Grade: Distinction\n");
        }
        else if(avg >= 60) {
            printf("Grade: First Class\n");
        }
        else if(avg >= 50) {
            printf("Grade: Second Class\n");
        }
        else if(avg >= 35) {
            printf("Grade: Third Class\n");
        }
        else {
            printf("Result: Fail\n");
        }
    }

    return 0;
}
